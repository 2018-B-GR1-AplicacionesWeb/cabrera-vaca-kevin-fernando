<script>
    $(document).ready(function () {
        $.ajax({
            url: "/tweets.asp",
            dataType: "json",
            success: function (data) {
                for (tt in data) {
                    $('#twitter_feed_rgt').append('<p>' + replaceLinks(data[tt].text) + '</p>');
                }

            }
        });
    });

    function replaceLinks(text) {
        var exp = /(\b(https?|ftp|file):\/\/[-A-Z0-9+&@#\/%?=~_|!:,.;]*[-A-Z0-9+&@#\/%=~_|])/ig;
        text = text.replace(exp, "<a target=\"_blank\" rel=\"nofollow\" href='$1'>$1</a>");

        var exp2 = /(^|\s)@(\w+)/g;
        return text.replace(exp2, "$1<a target=\"_blank\" rel=\"nofollow\" href=\"https://www.twitter.com/$2\">@$2</a>");

    }
</script>

